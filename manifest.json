{
  "manifest_version": 2,
  "name": "AutoClips",
  "version": "0.1.0",
  "background": {
    "scripts":["js/jquery-3.6.0.min.js","js/notify.min.js","background.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "iconOn.png"

    }
  },
  "content_scripts": [
    {
      "matches": ["https://*.twitch.tv/*"],
      "js": ["js/jquery-3.6.0.min.js","js/notify.min.js","script.js"],
      "css": ["styles.css"]
    }
  ],
  "permissions": [
    "storage",

    "tabs"
    
  ]
  
}