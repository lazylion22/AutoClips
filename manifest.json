{
  "manifest_version": 2,
  "name": "Twitch Auto Clips",
  "version": "0.1.0",
  "background": {
    "scripts":["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://*.twitch.tv/*"],
      "js": ["script.js"],
      "css": ["styles.css"]
    }
  ],
  "permissions": [
    "tabs"
  ]
}